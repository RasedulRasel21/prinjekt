    },
    {
      "type": "richtext",
      "id": "subtext",
      "label": "Subtext"
    },
    {
      "type": "image_picker",
      "id": "image",
      "label": "Image"
    },
    {
      "type": "url",
      "id": "linkarea",
      "label": "Linkarea"
    },
    {
      "type": "text",
      "id": "linkarea_text",
      "label": "Linkarea Text",
