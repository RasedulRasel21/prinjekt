
      <div class="splitscreen-column left">
        <div class="slider-description-wrap">
          <div class="slider-title-wrap">
            <h1 class="heading_h1">{{ section.settings.heading }}</h1>
          </div>
          <div class="slider-copy-wrap">
            <p class="text_normal left">{{ section.settings.subtext | newline_to_br }}</p>
          </div>
          <div class="slider-btn-wrap">
            <a href="{{ section.settings.text_link }}" data-w-id="645e68af-3972-aa15-2005-376d0ac851c2" class="button w-button">{{ section.settings.text_link_text }}</a>
          </div>
        </div>
      </div>
      <div class="splitscreen-column right">
        <div class="unsplash-copyright"></div>
        <div class="gallery-slider-mouse-stop"></div>
        <div data-delay="6000" data-animation="fade" class="gallery-slider w-slider" data-autoplay="true" data-easing="ease" data-hide-arrows="false" data-disable-swipe="false" data-autoplay-limit="0" data-nav-spacing="3" data-duration="800" data-infinite="true">
          <div class="gallery-mask w-slider-mask">
            <div class="gallery-slide w-slide">{%- if section.settings.img_header1 == nil -%}<img class="gallery-image" src="{{ 'bild_header_1.webp' | asset_url }}" alt="3D Druck Service aus Hannover" loading="lazy">{%- else -%}<img class="gallery-image" src="{{ section.settings.img_header1.src | img_url: 'master' }}" alt="{{ section.settings.img_header1.alt | escape }}" loading="lazy" srcset="{{ section.settings.img_header1 | img_url: '1920x' }} 1920w,{{ section.settings.img_header1 | img_url: '1600x' }} 1600w,{{ section.settings.img_header1 | img_url: '1366x' }} 1366w,{{ section.settings.img_header1 | img_url: '1024x' }} 1024w,{{ section.settings.img_header1 | img_url: '768x' }} 768w,{{ section.settings.img_header1 | img_url: '640x' }} 640w">{%- endif -%}</div>
            <div class="gallery-slide w-slide">{%- if section.settings.img_header2 == nil -%}<img class="gallery-image" src="{{ 'bild_header_1.webp' | asset_url }}" alt="Hochwertiger 3D-Druck Deutschland" loading="lazy">{%- else -%}<img class="gallery-image" src="{{ section.settings.img_header2.src | img_url: 'master' }}" alt="{{ section.settings.img_header2.alt | escape }}" loading="lazy" srcset="{{ section.settings.img_header2 | img_url: '1920x' }} 1920w,{{ section.settings.img_header2 | img_url: '1600x' }} 1600w,{{ section.settings.img_header2 | img_url: '1366x' }} 1366w,{{ section.settings.img_header2 | img_url: '1024x' }} 1024w,{{ section.settings.img_header2 | img_url: '768x' }} 768w,{{ section.settings.img_header2 | img_url: '640x' }} 640w">{%- endif -%}</div>
            <div class="gallery-slide w-slide"><img class="gallery-image" src="{{ 'bild_header_1.webp' | asset_url }}" alt="Individueller 3D-Druck" loading="lazy" img-header3=""></div>
          </div>
          <div class="display-none w-slider-arrow-left">
            <div class="w-icon-slider-left"></div>
          </div>
          <div class="display-none w-slider-arrow-right">
            <div class="w-icon-slider-right"></div>
          </div>
          <div class="display-none w-slider-nav w-round"></div>
        </div>
      </div>
    

<script>var currentSection = document.currentScript.closest('.shopify-section');currentSection.setAttribute("class","slider-stage-splitscreen");</script>
        
{% schema %}
{
  "name": "Header-Geschäftskunden",
  "tag": "div",
  "class": "slider-stage-splitscreen",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Heading",
      "default": "Der passende Druck für Ihre Firma!"
    },
    {
      "type": "text",
      "id": "text_button",
      "label": "Text-Button",
      "default": "Jetzt einen 3D-Druck bestellen"
    },
    {
      "type": "textarea",
      "id": "subtext",
      "label": "Subtext",
      "default": "Bei uns steht der Kunde im Mittelpunkt – wir fertigen für jeden den passenden Druck an. Ob Sie ein Unternehmen sind, das präzise Bauteile benötigt, oder ein Privatkunde, der ein individuelles Design verwirklichen möchte – wir bieten maßgeschneiderte Lösungen für Ihre spezifischen Anforderungen."
    },
    {
      "type": "image_picker",
      "id": "img_header1",
      "label": "IMG-Header1"
    },
    {
      "type": "image_picker",
      "id": "img_header2",
      "label": "IMG-Header2"
    },
    {
      "type": "url",
      "id": "text_link",
      "label": "Text-Link"
    },
    {
      "type": "text",
      "id": "text_link_text",
      "label": "Text-Link Text",
      "default": "{{ section.settings.text_button }}"
    }
  ]
}
{% endschema %}